{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "yarn",
  "reporters": [
    "html",
    "clear-text",
    "json",
    "progress"
  ],
  "mutate": [
    "src/**/*.js",
    "!src/plugin-utils.js",
    "!src/utils/constants.js",
    "index.js",
    "babel.js"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "jest": {
    "projectType": "custom",
    "configFile": "./jest.config.json"
  }
}
